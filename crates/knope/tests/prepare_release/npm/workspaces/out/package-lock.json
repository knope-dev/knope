{
  "name": "monorepo",
  "lockfileVersion": 3,
  "requires": true,
  "packages": {
    "": {
      "name": "monorepo",
      "workspaces": [
        "first",
        "others/*"
      ]
    },
    "first": {
      "name": "firstPackage",
      "version": "0.2.0",
      "dependencies": {
        "secondPackage": "2.1.0",
        "thirdPackage": "3.0.1"
      }
    },
    "node_modules/firstPackage": {
      "resolved": "first",
      "link": true
    },
    "node_modules/secondPackage": {
      "resolved": "others/second",
      "link": true
    },
    "node_modules/thirdPackage": {
      "resolved": "others/third",
      "link": true
    },
    "others/second": {
      "name": "secondPackage",
      "version": "2.1.0",
      "devDependencies": {
        "thirdPackage": "3.0.1"
      }
    },
    "others/third": {
      "name": "thirdPackage",
      "version": "3.0.1"
    }
  }
}